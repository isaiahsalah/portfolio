{
    "routes": [
        {
            "src": "/images/(.*)",
            "dest": "/images/$1",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/index.html",
            "status": 200
        }
    ]
}